
package com.nimo.show.view;

import java.util.ArrayList;

public abstract class IGroupItem implements Cloneable{

    private ArrayList<IChildItem> chirldList;

    private String name;
    
    private int groupPosition;

    public ArrayList<IChildItem> getChirldList() {
        return chirldList;
    }

    public void setChirldList(ArrayList<IChildItem> chirldList) {
        this.chirldList = chirldList;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getGroupPosition() {
        return groupPosition;
    }

    public void setGroupPosition(int groupPosition) {
        this.groupPosition = groupPosition;
    }

    @Override
    protected Object clone() throws CloneNotSupportedException {
        // TODO Auto-generated method stub
        return super.clone();
    }
    
    
    
}
